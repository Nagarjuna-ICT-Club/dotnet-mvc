@model MvcMovie.Models.MovieViewModel;


<div class="card">
    <div class="card-header">
        Movie Details
    </div>
    <div class="card-body">
        <h4>ID :  @Model.id </h4>
        <h4>Genre: @Model.genre </h4>
        <h4>Title: @Model.Title </h4>
        <h4>Release Date: @Model.releaseDate </h4>
    </div>
</div>